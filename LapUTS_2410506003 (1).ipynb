        "n = int(input(\"Jumlah baris: \"))\n",
        "i = 1\n",
        "while i <= n:\n",
        "    j = 1\n",
        "    while j <= i:\n",
        "        print(i*j, end=\" \")\n",
        "        j += 1\n",
        "    print()\n",
        "    i += 1"
      ],
